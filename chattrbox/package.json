{
  "name": "chattrbox",
  "version": "0.0.0",
  "description": "chat client application",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "browserify -d app/scripts/src/main.js -o app/scripts/dist/main.js",
    "watch": "watchify -v -d app/scripts/src/main.js -o app/scripts/dist/main.js"
  },
  "browserify": {
    "debug": true,
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "babel-preset-env"
          ]
        }
      ]
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babelify": "^8.0.0",
    "browserify": "^17.0.0",
    "nodemon": "^3.0.1",
    "parcel": "^2.10.2",
    "watchify": "^4.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es": "^1.0.0",
    "ws": "^8.14.2"
  }
}
